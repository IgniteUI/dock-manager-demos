{
  "name": "stream-manager",
  "private": true,
  "version": "0.0.0",
  "description": "This project provides example of Dock Manager Web component",
  "author": "Infragistics",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run watch-theme\"",
    "build": "npm run compile-theme && tsc && vite build",
    "compile-theme": "sass --load-path=node_modules src/styles/theme.scss:src/styles/theme.css",
    "watch-theme": "sass --watch --load-path=node_modules src/styles:src/styles src/styles/theme.scss:src/styles/theme.css"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.13",
    "@infragistics/igniteui-dockmanager": "^1.16.1",
    "igniteui-theming": "^19.1.1",
    "igniteui-webcomponents": "6.1.1-alpha.0",
    "marked": "^16.0.0",
    "lit": "^3.2.1",
    "sass": "^1.86.3"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "typescript": "~5.7.2",
    "vite": "^6.3.1"
  }
}
